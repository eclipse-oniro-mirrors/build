import("//build/ohos.gni")
import("//build/test.gni")

ohos_rust_systemtest("test_rust_st") {
  module_out_path = "rust_build_test/test_rust_st"
  sources = [ "test/test.rs" ]
  deps = [ ":test_rust_st_add" ]
}

ohos_rust_shared_library("test_rust_st_add") {
  sources = [ "src/lib.rs" ]
  crate_name = "test_rust_st_add"
  crate_type = "dylib"
}