import("//build/ohos.gni")
import("//build/test.gni")

ohos_rust_unittest("test_rust_ut") {
  module_out_path = "rust_build_test/test_rust_ut"
  sources = [ "src/main.rs" ]
  deps = [ ":test_rust_ut_add" ]
}

ohos_rust_shared_library("test_rust_ut_add") {
  sources = [ "src/main.rs" ]
  crate_name = "test_rust_ut_add"
  crate_type = "dylib"
}

